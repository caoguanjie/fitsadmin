{
    "name": "fitsadmin",
    "version": "1.0.1",
    "scripts": {
        "dev": "vite serve --mode dev",
        "build:prod": "vue-tsc --noEmit && vite build --mode prod",
        "build:dev": " vite build --mode dev",
        "build:github": " vite build --mode github",
        "jenkins": "npx --max_old_space_size=4096 vite build --mode dev",
        "serve": "vite preview",
        "lint": "eslint src/**/*.{ts,js,vue} --fix",
        "prepare": "husky install",
        "prettier": "prettier --write .",
        "docs:build": " vuepress build docs --clean-cache",
        "docs:clean-dev": "vuepress dev docs --clean-cache",
        "docs:dev": "vuepress dev docs",
        "docs:github": "cross-env NODE_ENV=github vuepress build docs",
        "build:lib": "vite build --config ./FitsAdminUI/lib.config.ts && cd FitsAdminUI && npm version patch && npm publish"
    },
